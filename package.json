{
  "name": "d6",
  "version": "0.1.7",
  "description": "An isomorphic JavaScript library for conditional tier rendering",
  "dependencies": {
    "jymin": "^0.3.5"
  },
  "devDependencies": {
    "zeriousify": "^0.1.10",
    "exam": "^0.0.8",
    "istanbul": "0.3.0",
    "coveralls": "2.11.1",
    "figlet": "1.1.0"
  },
  "keywords": [
    "d6"
  ],
  "main": "d6.js",
  "engines": {
    "node": ">=0.2.6"
  },
  "scripts": {
